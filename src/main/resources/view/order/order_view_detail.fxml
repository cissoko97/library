<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Text fx:id="title_text" fontSmoothingType="LCD" strokeType="OUTSIDE" strokeWidth="0.0" text="title commande" BorderPane.alignment="CENTER">
         <font>
            <Font name="DejaVu Sans Bold" size="43.0" />
         </font>
      </Text>
   </top>
   <center>
      <TableView fx:id="tableView" editable="true" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <right>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="acceptOrder_btn" mnemonicParsing="false" prefHeight="54.0" prefWidth="100.0" text="Button">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
         <BorderPane.margin>
            <Insets left="4.0" right="2.0" />
         </BorderPane.margin>
      </VBox>
   </right>
</BorderPane>
